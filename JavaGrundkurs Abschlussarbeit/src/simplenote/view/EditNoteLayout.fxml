   <children>
      <HTMLEditor fx:id="noteText" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" layoutX="10.0" layoutY="50.0" prefHeight="400.0" prefWidth="980.0" />
      <TextField fx:id="noteTitle" layoutX="10.0" layoutY="15.0" promptText="Titel" />
      <ToggleButton fx:id="noteShare" layoutX="937.0" layoutY="15.0" mnemonicParsing="false" text="Teilen" />
      <Button layoutX="912.0" layoutY="460.0" mnemonicParsing="false" onAction="#saveNote" text="Speichern" />
      <Button layoutX="10.0" layoutY="460.0" mnemonicParsing="false" onAction="#addFile" text="Datei hinzufügen" />
      <Button layoutX="135.0" layoutY="460.0" mnemonicParsing="false" onAction="#addLink" text="Link hinzufügen" />
   </children>
</AnchorPane>
